{
  "RemoteEndpoint": "http://localhost:5000",
  "SpecLocation": "https://raw.githubusercontent.com/HylandSoftware/Hot-Potato/master/test/RawPotatoSpec.yaml",
  "SpecToken": "",
  "HttpClientSettings": {
    "IgnoreClientHttpsCertificateValidationErrors": "false"
  },
  "ForwardProxy": {
    "Enabled": "false",
    "ProxyAddress": "http://localhost:8888",
    "BypassOnLocal": "false"
  },
  "exclude": [
    "**/bin",
    "**/bower_components",
    "**/jspm_packages",
    "**/node_modules",
    "**/obj",
    "**/platforms"
  ],
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}